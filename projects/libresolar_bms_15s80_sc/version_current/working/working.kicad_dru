(version 1)
(rule "class 0:0"
  (condition "A.NetClass == 'default' && B.NetClass == 'Default'")
  (constraint clearance (min 0.1524mm)))

(rule "class 1:1"
  (condition "A.NetClass == 'power' && B.NetClass == 'power'")
  (constraint clearance (min 0.1524mm)))

(rule "class 2:2"
  (condition "A.NetClass == 'VIN' && B.NetClass == 'VIN'")
  (constraint clearance (min 0.1524mm)))
